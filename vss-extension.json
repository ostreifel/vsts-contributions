{
    "manifestVersion": 1,
    "id": "contributions",
    "version": "1.0.4",
    "name": "Contributions Graph",
    "scopes": [ "vso.work", "vso.work_write", "vso.code" ],
    "description": "View account contributions.",
    "publisher": "ottostreifel",
    "icons": {
        "default": "img/logo.png"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud"
        }
    ],
    "tags": [
        "Work Items",
        "Work Item",
        "Git",
        "Pull Request",
        "Commit",
        "Visualization",
        "Contributions"
    ],
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/ostreifel/vsts-contributions"
    },
    "branding": {
        "color": "rgb(220, 235, 252)",
        "theme": "light"
    },
    "files": [
        {
            "path": "img",
            "addressable": true
        },
        {
            "path": "dist",
            "addressable": true
        },
        {
            "path": "contributions.html",
            "addressable": true
        }
    ],
    "categories": [
        "Collaborate"
    ],
    "contributions": [
        {
            "id": "contributions-hub",
            "type": "ms.vss-web.hub",
            "description": "View code and work item contributions.",
            "targets": [
                "ms.vss-work-web.work-hub-group"
                ],
            "properties": {
                "group": "contributed",
                "name": "Contributions",
                "uri": "contributions.html"
            }
        }
    ]
}
